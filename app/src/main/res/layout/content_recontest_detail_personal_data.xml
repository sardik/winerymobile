<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card_recontest_detail_personal_data"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        app:strokeColor="@android:color/black"
        app:cardCornerRadius="@dimen/default_padding_medium"
        app:strokeWidth="1dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:padding="@dimen/default_margin_small"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                android:textSize="@dimen/font_size_14"
                android:layout_alignParentStart="true"
                android:text="Data Pribadi"/>

            <ImageView
                android:id="@+id/ivChevronDetailPersonalData"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:src="@drawable/ic_expand_more_black_24dp"/>

        </RelativeLayout>

    </com.google.android.material.card.MaterialCardView>

    <RelativeLayout
        android:id="@+id/parent_container_item_data_car"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_container_item_data_card"
        android:visibility="gone"
        android:layout_marginTop="-16dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/default_margin_small"
            android:orientation="vertical">

            <TextView
                android:layout_marginTop="@dimen/default_margin_small"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                android:text="Nama"/>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/ti_name"
                style="@style/text_input_basic">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/et_name"
                    style="@style/edit_text_input"
                    android:hint="Nama" />
            </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:layout_marginTop="@dimen/default_margin_small"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                android:text="NIK"/>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/ti_nik"
                style="@style/text_input_basic">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/et_nik"
                    style="@style/edit_text_input"
                    android:inputType="number"
                    android:maxLength="16"
                    android:hint="NIK" />
            </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:layout_marginTop="@dimen/default_margin_small"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                android:text="Tempat Lahir"/>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/ti_tempat_lahir"
                style="@style/text_input_basic">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/et_tempat_lahir"
                    style="@style/edit_text_input"
                    android:hint="Tempat Lahir" />
            </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:layout_marginTop="@dimen/default_margin_small"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/black"
                android:text="Tanggal lahir"/>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/ti_dob"
                style="@style/text_input_basic">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/et_dob"
                    style="@style/edit_text_input"
                    android:hint="dd/mm/yyyy" />
            </com.google.android.material.textfield.TextInputLayout>

